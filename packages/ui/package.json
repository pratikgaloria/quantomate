{
    "name": "@quantomate/ui",
    "version": "0.1.0",
    "private": true,
    "description": "Backtest visualization UI",
    "scripts": {
        "fetch-strategies": "node scripts/fetch-strategies.js",
        "dev:server": "cd server && npm run dev",
        "dev:client": "cd client && npm run dev",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "build:client": "cd client && npm run build",
        "build": "npm run fetch-strategies && npm run build:client"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}